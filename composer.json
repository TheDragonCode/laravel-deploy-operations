{
    "name": "dragon-code/laravel-migration-actions",
    "description": "Performing actions with saving the list of called files",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "migration",
        "migrations",
        "action",
        "actions",
        "dragon-code",
        "dragon",
        "andrey-helldar"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/laravel-migration-actions/issues",
        "source": "https://github.com/TheDragonCode/laravel-migration-actions"
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "dragon-code/contracts": "^2.9",
        "illuminate/console": "^6.0 || ^7.0 || ^8.0",
        "illuminate/database": "^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "symfony/console": "^4.3 || ^5.0"
    },
    "require-dev": {
        "dragon-code/laravel-support": "^3.1",
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "ramsey/uuid": "^3.7 || ^4.0"
    },
    "conflict": {
        "andrey-helldar/laravel-actions": "*",
        "dragon-code/laravel-actions": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\LaravelActions\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DragonCode\\LaravelActions\\ServiceProvider"
            ]
        }
    }
}
